                                    <td class="grey-cell" style="width :20%;padding:0;text-align:center;" colspan="2">
                                        <strong style="font-size:10px;">rechts [kN]</strong>
                                    </td>
                                    <td class="grey-cell" style="width :20%;padding:0;text-align:center;" colspan="2">
                                        <strong style="font-size:10px;">max.Fehler [%]</strong>
                                    </td>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td style="padding:0 0 0 10px;">
                                        <span style="font-size:10px;">Messbereich: vorne/hinten oder</span>
                                    </td>
                                    <td style="padding:0 0 0 10px;">
                                        <span style="font-size:10px;">klein</span>
                                    </td>
                                    <td style="padding:0 0 0 10px;">
                                        <span style="font-size:10px;">groß</span>
                                    </td>
                                    <td style="padding:0 0 0 10px;">
                                        <span style="font-size:10px;">klein</span>
                                    </td>
                                    <td style="padding:0 0 0 10px;">
                                        <span style="font-size:10px;">groß</span>
                                    </td>
                                    <td style="padding:0 0 0 10px;">
                                        <span style="font-size:10px;">klein</span>
                                    </td>
                                    <td style="padding:0 0 0 10px;">
                                        <span style="font-size:10px;">groß</span>
                                    </td>
                                </tr>
