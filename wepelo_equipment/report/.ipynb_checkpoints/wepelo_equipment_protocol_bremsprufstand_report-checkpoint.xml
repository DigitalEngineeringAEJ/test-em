<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="report_equipment_protocol_bremsprufstand">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <style>
                        .checkbox-xl .custom-control-label::before,
                        .checkbox-xl .custom-control-label::after {
                        width: 1rem;
                        height: 1rem;
                        }
                    </style>
                    <div class="page">
                        <div class="oe_structure"/>
                        <h4>
                            <strong>Prüfbericht</strong>
                            <br></br>
                            <span style="font-size:10px;">Stückprüfung eines Bremsprüfstands
                                (nach StVZO Deutschland)
                            </span>
                        </h4>
                        <div class="row col-12" style="margin-left:5px%;">
                            <div class="col-8" style="border:1px solid black;">
                                <strong style="font-size:10px">Anschrift der Firma, die den Bremsprüfstand betreibt:
                                </strong>
                                <br></br>
                                <span t-field="o.customer_id.name" style="font-size:10px"/><t t-if="o.customer_id.street">, <span t-field="o.customer_id.street" style="font-size:10px"/></t><t t-if="o.customer_id.street2 or o.customer_id.zip">, <span t-field="o.customer_id.street2" style="font-size:10px"/><span t-field="o.customer_id.zip" style="font-size:10px"/></t><t t-if="o.customer_id.city">, <span t-field="o.customer_id.city" style="font-size:10px"/></t>
                                <table class="table table-borderless"
                                       style="margin-top:0px;margin-bottom:0px;padding:0px;">
                                    <tr style="margin:0px;padding:0px;">
                                        <td style="margin:0px;padding:0px;">
                                            <strong style="font-size:10px">Datum der Prüfung:</strong>
                                            <span t-field="o.mail_activity_id.schedule_date"
                                                  t-options='{"format": "dd.MM.yyyy"}' style="font-size:10px"/>
                                        </td>
                                        <td style="margin:0px;padding:0px;">
                                            <strong style="font-size:10px;">Gerätetyp:</strong>
                                            <span t-field="o.equipment_id.category_id.name" style="font-size:10px;"/>
                                        </td>
                                    </tr>
                                </table>
                                <div class="custom-control custom-checkbox checkbox-xl col-12">
                                    <t t-if="o.mail_activity_id.is_torque_measurement">
                                        <input type="checkbox" class="custom-control-input"
                                               id="checkbox_torque_measurement" checked="checked"/>

                                        <label class="custom-control-label" for="checkbox_torque_measurement">
                                        </label>
                                        <span style="font-size:10px">Rollenbremsprüfstand auf der Basis der
                                            Drehmomentmessung
                                        </span>
                                    </t>
                                    <t t-if="not o.mail_activity_id.is_torque_measurement">
                                        <input type="checkbox" class="custom-control-input"
                                               id="checkbox_regular"/>
                                        <label class="custom-control-label" for="checkbox_torque_measurement"
                                        >
                                        </label>
                                        <span style="font-size:10px">Rollenbremsprüfstand auf der Basis der
                                            Drehmomentmessung
                                        </span>
                                    </t>
                                </div>
                                <div class="custom-control custom-checkbox checkbox-xl col-12">
                                    <t t-if="o.mail_activity_id.is_thermal_power">
                                        <input type="checkbox" class="custom-control-input"
                                               id="checkbox_thermal_power" checked="checked"/>

                                        <label class="custom-control-label" for="checkbox_thermal_power">
                                        </label>
                                        <span style="font-size:10px">Rollenbremsprüfstand auf der Basis der
                                            Wirkleistungsmessung
                                        </span>
                                    </t>
                                    <t t-if="not o.mail_activity_id.is_thermal_power">
                                        <input type="checkbox" class="custom-control-input"
                                               id="checkbox_thermal_power"/>
                                        <label class="custom-control-label" for="checkbox_thermal_power"/>
                                        <span style="font-size:10px">Rollenbremsprüfstand auf der Basis der
                                            Wirkleistungsmessung
                                        </span>

                                    </t>
                                </div>
                                <div class="custom-control custom-checkbox checkbox-xl col-12">
                                    <t t-if="o.mail_activity_id.is_plate">
                                        <input type="checkbox" class="custom-control-input"
                                               id="checkbox_plate" checked="checked"/>

                                        <label class="custom-control-label" for="checkbox_plate"
                                        />
                                        <span style="font-size:10px">Plattenbremsprüfstand</span>
                                    </t>
                                    <t t-if="not o.mail_activity_id.is_plate">
                                        <input type="checkbox" class="custom-control-input"
                                               id="checkbox_plate"/>
                                        <label class="custom-control-label" for="checkbox_plate"
                                        />
                                        <span style="font-size:10px">Plattenbremsprüfstand</span>
                                    </t>
                                </div>
                                <table class="table table-borderless table-sm"
                                       style="margin-top:0px;margin-bottom:0px;padding:0px;">
                                    <tr style="margin-bottom:0px;padding:0px;">
                                        <td style="margin-bottom:0px;padding:0px;">
                                            <strong style="font-size:10px">Hersteller:</strong>
                                            <span t-field="o.manufacturer_id" style="font-size:10px"/>
                                        </td>
                                        <td style="margin-bottom:0px;padding:0px;">
                                            <strong style="font-size:10px">Fabrikat-Nr:</strong>
                                            <span t-field="o.equipment_id.serial_no" style="font-size:10px"/>
                                        </td>
                                    </tr>
                                </table>
                                <table class="table table-borderless table-sm"
                                       style="margin-top:0px;margin-bottom:0px;padding:0px;">
                                    <tr style="padding:0;margin:0;">
                                        <td style="padding:0;margin:0;">
                                            <strong style="font-size:10px">Baujahr / Inbetriebnahme:</strong>
                                            <span style="font-size:10px">Monat:
                                                <span t-field="o.date"
                                                      t-options='{"format": "M"}'/>
                                                /Jahr:
                                                <span t-field="o.date"
                                                      t-options='{"format": "YYYY"}'/>
                                            </span>
                                        </td>
                                    </tr>
                                    <tr style="padding:0;margin:0;">
                                        <td style="padding:0;margin:0;">
                                            <strong style="font-size:10px">Bauartprüfung nach der Richtlinie von 2011
                                            </strong>
                                        </td>
                                    </tr>
                                </table>
                            </div>
                            <div class="col-4" style="border:1px solid black;">
                                <strong style="font-size:10px">Prüfmittel Seriennummern</strong>
                                <table class="table table-borderless table-sm" style="margin:0px;padding:0px;">
                                    <tr>
                                        <td>
                                            <span style="font-size:10px">Kalibriervornichtung:</span>
                                            <span style="font-size:10px"
                                                  t-field="o.mail_activity_id.calibration_device"/>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <span style="font-size:10px">siehe Ks</span>
                                            <span style="font-size:10px" t-field="o.mail_activity_id.ks"/>
                                        </td>
                                    </tr>
                                </table>
                                <p style="margin-top:50px;font-size:10px;text-align:center;">Alle verwendeten Prüfmitel
                                    sind
                                    <br></br>
                                    auf nationale oder internationale
                                    <br></br>
                                    Normen rückführbar
                                </p>
                            </div>


                        </div>
                        <strong style="font-size:10px">Ergebnis der Stückprüfung- Messtechnische Eigenschaften
                            <br></br>
